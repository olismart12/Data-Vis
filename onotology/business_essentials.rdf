    <owl:Class rdf:about="http://example.com/BusinessAnalysisEssentials#BusinessAnalyst">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://example.com/BusinessAnalysisEssentials#Role"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://example.com/BusinessAnalysisEssentials#elicits"/>
