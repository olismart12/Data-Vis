                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://example.com/BusinessAnalysisEssentials#manages"/>
                        <owl:someValuesFrom rdf:resource="http://example.com/BusinessAnalysisEssentials#Requirement"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://example.com/BusinessAnalysisEssentials#recommends"/>
                        <owl:someValuesFrom rdf:resource="http://example.com/BusinessAnalysisEssentials#Solution"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://example.com/BusinessAnalysisEssentials#understands"/>
                        <owl:someValuesFrom rdf:resource="http://example.com/BusinessAnalysisEssentials#LifeCycle"/>
                    </owl:Restriction>
